{
  "compilerOptions": {
    "target": "es6",
    "lib": ["DOM", "ES2020"],
    "module": "ESNext",
    "allowJs": true,
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true
  }
}
